// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model Product {
  id      String  @id
  name    String
}

model Order {
  id            String    @id
  subject       String
  status        String
  remark        String
  editedDate    String?
  effectiveDate String
}

model User {
  id       String  @id
  name     String
  lastName String
}
